    "# Programmatic search: find Stable Diffusion pipelines by CompVis\n",
    "from huggingface_hub import HfApi\n",
    "api = HfApi()\n",
    "\n",
    "models = list(api.list_models(\n",
    "    author=\"CompVis\",\n",
    "    tags=[\"diffusers:StableDiffusionPipeline\", \"text-to-image\"],\n",
    "    sort=\"downloads\",\n",
    "    direction=-1,\n",
    "    limit=5\n",
    "))\n",
    "\n",
    "top = models[0]\n",
    "print(top.id)  # model repository ID, e.g., \"runwayml/stable-diffusion-v1-5\" (example)"
   ]
